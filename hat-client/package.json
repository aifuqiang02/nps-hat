    "element-plus": "^2.8.3",
    "node-machine-id": "^1.1.12",
    "osinfo": "^0.6.2",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.1",
    "@rushstack/eslint-patch": "^1.5.1",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "electron": "^26.3.0",
    "electron-builder": "^24.6.4",
    "electron-vite": "^2.3.0",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.17.0",
    "less": "^4.2.0",
